<body class="az-body">

  <div class="az-signup-wrapper">
    <div class="az-column-signup-left">
      <div>
        <i class="typcn typcn-chart-bar-outline"></i>
        <h1 class="az-logo">Talisman</h1>
        <h5>Responsive Modern Dashboard &amp; Admin Template</h5>
      </div>
